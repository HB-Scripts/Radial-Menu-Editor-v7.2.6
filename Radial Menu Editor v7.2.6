			This.TabBitmaps[ A_Index ].SelectedBitmap := This._Create_TabBitmap_Graphics( A_Index , color , header , subcats , icon , scalefactor, 1 )
		}
	}
	_Dispose_ALL_TabBitmaps(){
		Loop, 16	{
			Gdip_DisposeImage( This.TabBitmaps[ A_Index ].DefaultBitmap() )
			Gdip_DisposeImage( This.TabBitmaps[ A_Index ].SelectedBitmap() )
		}
		This.TabBitmaps := ""
	}
	UpdateTabs( selectedTab := 1 ){
		static lastTab := ""
		if( lastTab )
