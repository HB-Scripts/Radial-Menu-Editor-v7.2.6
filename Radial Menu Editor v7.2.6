	_Hover( l , w , msg , hwnd ){
		
		if( !This.HoveredControl && CategoryEditWindowClass.Window.ControlHandles[ hwnd ].Name ){
			This.HoveredControl := hwnd
			cc := CategoryEditWindowClass.Window.ControlHandles[ hwnd ]
			cc.DrawBitmap( CategoryEditWindowClass[ "_Create_Graphics_" cc.Name ]( This.Scale , 1 ) , { X: 0 * This.Scale , Y: 0 * This.Scale , W: cc.W * This.Scale , H: cc.H * This.Scale } , dispose := 1 , autoUpdate := 1 )
			timer := This.HoverTimer
			SetTimer, % timer , 100
			;~ ToolTip, % "Tip:`n" cc.Name
		}
	}
	_WatchLeaveHover(){
		MouseGetPos,,,, ctrl , 2
		if( ctrl != This.HoveredControl ){
			cc := CategoryEditWindowClass.Window.ControlHandles[ This.HoveredControl ]
			cc.DrawBitmap( CategoryEditWindowClass[ "_Create_Graphics_" cc.Name ]( This.Scale , 0 ) , { X: 0 * This.Scale , Y: 0 * This.Scale , W: cc.W * This.Scale , H: cc.H * This.Scale } , dispose := 1 , autoUpdate := 1 )
			timer := This.HoverTimer
			SetTimer, % timer , Off
			sleep, 30
			This.HoveredControl := ""
		}
	}
	_Click( l , w , msg , hwnd ){
		cc := This.CategoriesWindow.Window.ControlHandles[ Hwnd ]
		if( cc.Index ){
			CoordMode, mouse, client
			MouseGetPos, x, y, , ctrl , 2
			x -= This.CategoriesWindow.Window.X
			y -= This.CategoriesWindow.Window.Y
			if( cc.Index >= 2 ){
				This.SelectedStepList := 1
				This.StepsListWindow.Window.Hide()
				This.StepsMainWindow.Window.Hide()
				This.SelectedCategory := cc.Index - 1
				This.CategoriesWindow.UpdateTabs( cc.Index - 1 )
				if( This.Data.Categories.Headers[ This.SelectedCategory ] != "" ){
					SubCategoryWindowClass.Window.Show()
					This.QuickListWindow.Window.Show()
					cat := This.SelectedCategory
					subCat := This.SelectedSubCategory
					if( This.Data.Categories[ cat ].Headers[ subCat ] ){
						
						This.StepsListWindow.Window.ClearWindow()
						This.StepsListWindow.Window.DrawBitmap( This.StepsListWindow._Create_Graphics_StepsListControl( ScaleFactor := 1 ) , { X: 0 , Y: 0 , W: This.StepsListWindow.Window.W , H: This.StepsListWindow.Window.H } , dispose := 1 , autoUpdate := 1 )
						This.StepsListWindow.Window.Show()
						step := This.SelectedStepList
						header := This.Data.Categories[ cat ][ subCat ][ step ].StepHeader
						if( header != "" ){
							This.StepsMainWindow.Window.Show()
						}
					}
					
					
					
					Loop, 9	{
						icon 	:= This.Data.Categories[ This.SelectedCategory ].Icons[ A_Index ]
						header 	:= This.Data.Categories[ This.SelectedCategory ].Headers[ A_Index ]
						pBitmap := This.SubCategoriesWindow._Create_Graphics_IconButton( ( This.SelectedSubCategory = A_Index ) ? ( 1 ) : ( 0 ) , header , icon , This.Scale )
						dd := This.SubCategoriesWindow.Window.Controls.Icons[ A_Index ]
						dd.ClearWindow()
						dd.DrawBitmap( pBitmap , { X: 0 , Y: 0 , W: dd.W , H: dd.H } , 1 , 1 )
					}
					dd := This.SubCategoriesWindow.Window.Controls.TextControl
					pBitmap := This.SubCategoriesWindow._Create_Graphics_TextControl( ScaleFactor := 1 )
					dd.DrawBitmap( pBitmap , { X: 0 , Y: 0 , W: dd.W , H: dd.H } , 1 , 1 )
					ee := This.QuickListWindow.Window
					ee.ClearWindow()
					ee.DrawBitmap( This.QuickListWindow._Create_Graphics_QuickListControl() , { X: 0 , Y: 0 , W: ee.W , H: ee.H } , 1 , 1 )
					
				}else{
					;~ SoundBeep, 555
					SubCategoryWindowClass.Window.Hide()
					This.QuickListWindow.Window.Hide()
					This.StepsListWindow.Window.Hide()
					This.StepsMainWindow.Window.Hide()
				}
			}else if( y >= 135 ){
				This.Goto_Edit_Category()
			}
