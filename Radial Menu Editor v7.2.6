WriteSubCategoryData( SelectedCategory , SelectedSubCategory ){
	local path := A_ScriptDir "\Radial Menu v7 Data.ini"
	local cc := RadialMenuEditor.Data.Categories[ SelectedCategory ]
	local header := cc.Headers[ SelectedSubCategory ]
	local icon := cc.Icons[ SelectedSubCategory ]
	local tip := cc.Tips[ SelectedSubCategory ]
	local OutputString := cc[ SelectedSubCategory ].OutputString
	
	ToolTip, Saving
	IniWrite, % header , % path , % "Category_" SelectedCategory "_Headers" , % SelectedSubCategory 
	IniWrite, % icon , % path , % "Category_" SelectedCategory "_Icons", % SelectedSubCategory 
	IniWrite, % tip , % path , % "Category_" SelectedCategory "_Tips", % SelectedSubCategory 
	IniWrite, % OutputString , % path , % "Category_" SelectedCategory "_" SelectedSubCategory "_OutputString" , OutputString
	ToolTip, Done `nSaving
	sleep, 600
	ToolTip
}
