	local path 		:= A_ScriptDir "\Radial Menu v7 Data.ini"
	local cc 		:= RadialMenuEditor.Data.Categories[ RadialMenuEditor.SelectedCategory ]
	local index 	:= RadialMenuEditor.SelectedQuickList
	local output	:= cc.QuickList[ index ]
	local Header 	:= cc.QuickListHeaders[ index ]
	ToolTip, Saving
	IniWrite, % output , % path , % "Category_" RadialMenuEditor.SelectedCategory "_QuickList", % Index 
	IniWrite, % header , % path , % "Category_" RadialMenuEditor.SelectedCategory "_QuickListHeaders", % Index 
	ToolTip,
}
